{
  "name": "ampd",
  "private": true,
  "scripts": {
    "format": "ng lint --fix && prettier --write \"src/**/*.scss\"",
    "warnings": "ng lint --silent --format=checkstyle > checkstyle-ts-result.xml",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "e2e": "ng e2e",
    "cypress:open": "ng run ampd:cypress-open",
    "cypress:run": "cypress run",
    "delete:reports": "rm cypress/results/* || true",
    "generate:reports": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json && marge -o reports/html mochawesome.json && rm mochawesome.json"
  },
  "dependencies": {
    "@angular/animations": "~20.1.2",
    "@angular/cdk": "^20.1.2",
    "@angular/common": "~20.1.2",
    "@angular/compiler": "~20.1.2",
    "@angular/core": "~20.1.2",
    "@angular/forms": "~20.1.2",
    "@angular/material": "^20.1.2",
    "@angular/platform-browser": "~20.1.2",
    "@angular/platform-browser-dynamic": "~20.1.2",
    "@angular/router": "~20.1.2",
    "@stomp/rx-stomp": "^2.0.1",
    "@stomp/stompjs": "^7.1.1",
    "filesize": "^10.1.6",
    "material-icons": "1.13.14",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "~20.1.1",
    "@angular/compiler-cli": "~20.1.2",
    "@cypress/schematic": "3.0.0",
    "@types/node": "^22.10.10",
    "angular-eslint": "20.1.1",
    "autoprefixer": "^10.4.21",
    "cypress": "latest",
    "cypress-multi-reporters": "^2.0.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "npm-check-updates": "^18.0.1",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.7",
    "typescript-eslint": "^8.35.0"
  }
}
