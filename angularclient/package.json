{
  "name": "ampd",
  "private": true,
  "scripts": {
    "format": "ng lint --fix && prettier --write \"src/**/*.scss\"",
    "warnings": "ng lint --silent --format=checkstyle > checkstyle-ts-result.xml",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "e2e": "ng e2e",
    "cypress:open": "ng run ampd:cypress-open",
    "cypress:run": "cypress run",
    "delete:reports": "rm cypress/results/* || true",
    "generate:reports": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json && marge -o reports/html mochawesome.json && rm mochawesome.json"
  },
  "dependencies": {
    "@angular/animations": "~19.1.3",
    "@angular/cdk": "^19.0.3",
    "@angular/common": "~19.1.3",
    "@angular/compiler": "~19.1.3",
    "@angular/core": "~19.1.3",
    "@angular/forms": "~19.1.3",
    "@angular/material": "^19.0.3",
    "@angular/platform-browser": "~19.1.3",
    "@angular/platform-browser-dynamic": "~19.1.3",
    "@angular/router": "~19.1.3",
    "@stomp/rx-stomp": "^2.0.0",
    "@stomp/stompjs": "^7.0.0",
    "filesize": "^10.1.4",
    "material-icons": "1.13.12",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "typescript": "5.5.4"
  },
  "devDependencies": {
    "@angular/build": "^19.0.5",
    "@angular/cli": "~19.1.4",
    "@angular/compiler-cli": "~19.1.3",
    "@cypress/schematic": "2.5.2",
    "@types/node": "^22.5.5",
    "angular-eslint": "19.0.2",
    "autoprefixer": "^10.4.20",
    "cypress": "latest",
    "cypress-multi-reporters": "^1.6.4",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "npm-check-updates": "^17.0.2",
    "postcss": "^8.4.40",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.7",
    "typescript-eslint": "8.0.0"
  }
}